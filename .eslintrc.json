{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:vue/essential",
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "comma-dangle": 0,
        "import/no-unresolved": "off",
        "semi": 0,
        "no-alert": "off",
        "no-console": "off",
        "no-plusplus": "off",
        "vars-on-top": 0,
        "guard-for-in": 0,
        "no-restricted-syntax": 0
    }
}